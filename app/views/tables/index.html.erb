<% @event.tables.each do |table| %>
  <ul>Table <%="#{@table_num} Seated: #{table.count}"%>
    <% table.all.each do |guest| %>
      <li><%="#{guest.first_name} #{guest.last_name}"%></li>
      <% @guest_count += 1 %>
    <%end%>
  </ul>
  <% @table_num += 1 %>
<% end %>

Total Guests: <%= "#{@guest_count}" %>
